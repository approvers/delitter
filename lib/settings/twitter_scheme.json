{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Delitter Bot Setting Json Scheme",
  "description": "Delitterの設定が如何にして行われるべきかを説明するスキーマです。",
  "type": "object",
  "additionalProperties": false,

  "required": [
    "consumer_api_key",
    "consumer_api_secret_key",
    "access_token",
    "access_token_secret"
  ],

  "properties": {
    "_comment": {},
    "consumer_api_key": {
      "type": "string"
    },
    "consumer_api_secret_key": {
      "type": "string"
    },
    "access_token": {
      "type": "string"
    },
    "access_token_secret": {
      "type": "string"
    }
  }

}